<div class="col-md-12">
  <table class="table table-hover">
    <thead>
      <tr>
        <td>Url</td>
        <td>Created_at</td>
      </tr>
    </thead>
    <tbody>
      <% @histories.each do |history| %>
        <tr>
          <td><%= history.url %></td>
          <td><%= history.created_at %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
